<%= form_for(@item) do |f| %>
<% if @item.errors.any? %>
<div id="error_explanation">
     <h2><%= pluralize(@item.errors.count, "error") %> prohibited this part from being saved:</h2>
     <ul>
          <% @item.errors.full_messages.each do |msg| %>
          <li>
               <%= msg %>
          </li>
          <% end %>
     </ul>
</div>
<% end %>
<div class="field">
 <table class="table table-bordered table-striped globle-table">
     <tr>
          <td>数据集</td>
          <td><%=@item.model%></td>
     </tr>
     <tr>
          <td>映射键号</td>
          <td><%=@item.map_key%></td>
     </tr>
     <tr>
          <td>映射键值</td>
          <td><%= @item.map_field_value%></td>
     </tr>
     <tr>
          <td>映射值</td>
          <td><%= f.text_field :map_value%></td>
     </tr>
     <tr>
          <td>访问号</td>
          <td><%= @item.access_key%></td>
     </tr>
       <tr>
               <td colspan="2">    <%= f.submit "提 交"%></td>
          </tr>
</table>
</div>
<% end %>
