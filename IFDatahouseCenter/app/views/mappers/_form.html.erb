<%= form_for(@item) do |f| %>
<% if @item.errors.any? %>
<div id="error_explanation">
     <h2><%= pluralize(@item.errors.count, "error") %> prohibited this part from being saved:</h2>
     <ul>
          <% @item.errors.full_messages.each do |msg| %>
          <li>
               <%= msg %>
          </li>
          <% end %>
     </ul>
</div>
<% end %>
<div class="field">
     <table>
          <tr>
               <td>访问号</td>
               <td><%= @item.access_key%></td>
          </tr>
          <tr>
               <td>接口映射号</td>
               <td><%= f.text_field :mapperNr%></td>
          </tr>
          <tr>
               <td>名称</td>
               <td><%= f.text_field :name%></td>
          </tr>
     </table>
</div>
<div class="actions">
     <%= f.submit "提 交"%>
</div>
<% end %>
