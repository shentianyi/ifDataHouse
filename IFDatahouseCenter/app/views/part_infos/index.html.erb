<h1>Listing <%= @model%></h1>

<table>
     <tr>
          <th>零件号</th>
          <th>名称</th> 
          <th>零件订单号</th>
          <th>单位生产时间</th> 
          <th>&nbsp;</th>
     </tr>
     <% @items.each do |item| %>
     <tr>
          <td><%= item.partNr%></td>
          <td><%= item.name%></td> 
          <td><%= item.orderNr%></td>
          <td><%= item.unitTime%></td> 
          <td><%= link_to '查看', item %>
          <%= link_to '编辑',send("edit_#{@model}_path",item) %>
          <%= link_to '删除', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
     </tr>
     <% end %>
</table>
<br />

<%= will_paginate @items%>
<%= link_to "新建零件数据", send("new_#{@model}_path") %>
<%= link_to "上传/更新零件数据", send("import_#{@model.pluralize}_path") %><br />
<%= link_to "下载", send("download_#{@model.pluralize}_path") %>